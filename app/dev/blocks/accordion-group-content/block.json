{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpbs/accordion-group-content",
  "title": "Accordion Content",
  "description": "Display an accordion content",
  "selectors": {
    "root": ".wpbs-accordion-group-content"
  },
  "attributes": {
    "uniqueId": {
      "type": "string"
    },
    "groupId": {
      "type": "string"
    }
  },
  "parent": [
    "wpbs/accordion-group-item"
  ],
  "usesContext": [
    "settings"
  ],
  "supports": {
    "html": false,
    "innerBlocks": true,
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "gradients": true
    },
    "spacing": {
      "padding": true,
      "margin": false,
      "selectors": {
        "root": ".wpbs-accordion-group-content",
        "wrapper": ".wpbs-accordion-group-content__wrapper"
      }
    },
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    },
    "typography": {
      "__experimentalFontFamily": true,
      "__experimentalFontWeight": true,
      "__experimentalTextTransform": true,
      "textAlign": true,
      "fontSize": true,
      "lineHeight": true
    }
  },
  "editorScript": "file:./block.js",
  "icon": "excerpt-view"
}


