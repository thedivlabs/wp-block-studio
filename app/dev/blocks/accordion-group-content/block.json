{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpbs/accordion-group-content",
  "title": "Accordion Content",
  "description": "Display an accordion content",
  "selectors": {
    "root": ".wpbs-accordion-group-content",
    "spacing": ".wpbs-accordion-group-content__wrapper"
  },
  "attributes": {
    "uniqueId": {
      "type": "string"
    }
  },
  "parent": [
    "wpbs/accordion-group-item"
  ],
  "supports": {
    "html": false,
    "innerBlocks": true,
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "gradients": true
    },
    "spacing": {
      "padding": true,
      "margin": false
    },
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "fontStyle": true,
      "fontWeight": true
    }
  },
  "editorScript": "file:./js/edit.js",
  "icon": "excerpt-view"
}


