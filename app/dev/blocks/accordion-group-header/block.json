{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpbs/accordion-group-header",
  "title": "Accordion Header",
  "description": "Display an accordion header",
  "selectors": {
    "root": ".wpbs-accordion-group-header"
  },
  "attributes": {
    "uniqueId": {
      "type": "string"
    }
  },
  "parent": [
    "wpbs/accordion-group-item"
  ],
  "allowedBlocks": [
    "core/heading",
    "core/paragraph"
  ],
  "supports": {
    "html": false,
    "innerBlocks": true,
    "dimensions": {
      "minHeight": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "fontStyle": true,
      "fontWeight": true
    },
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "gradients": true
    },
    "spacing": {
      "padding": true,
      "margin": false
    },
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    }
  },
  "editorScript": "file:./js/edit.js",
  "icon": "excerpt-view"
}


