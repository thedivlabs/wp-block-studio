{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpbs/accordion-group",
  "title": "Accordion Group",
  "description": "Display an accordion section",
  "selectors": {
    "root": ".wpbs-accordion-group"
  },
  "attributes": {
    "uniqueId": {
      "type": "string"
    },
    "wpbs-settings": {
      "type": "object"
    }
  },
  "styles": [
    {
      "name": "single",
      "label": "Single"
    }
  ],
  "allowedBlocks": [
    "wpbs/accordion-group-item"
  ],
  "supports": {
    "interactivity": true,
    "html": false,
    "innerBlocks": true,
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "gradients": true
    },
    "spacing": {
      "blockGap": [
        "vertical"
      ],
      "padding": true,
      "margin": true
    },
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "fontStyle": true,
      "fontWeight": true
    }
  },
  "editorScript": "file:./block.js",
  "viewScriptModule": "file:./view.js",
  "style": "file:./block.css",
  "icon": "excerpt-view"
}


