{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpbs/content-accordion",
  "title": "Content Accordion",
  "description": "Display an accordion section",
  "selectors": {
    "root": ".wpbs-content-accordion"
  },
  "attributes": {
    "uniqueId": {
      "type": "string"
    }
  },
  "styles": [
    {
      "name": "single",
      "label": "Single"
    }
  ],
  "supports": {
    "interactivity": true,
    "html": false,
    "innerBlocks": true,
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "gradients": true
    },
    "spacing": {
      "blockGap": [
        "vertical"
      ],
      "padding": true,
      "margin": true
    },
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    }
  },
  "editorScript": "file:./js/edit.js",
  "viewScriptModule": "file:./js/view.js",
  "style": "file:./js/edit.css",
  "icon": "excerpt-view"
}


