    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true,
      "__experimentalSkipSerialization": true
    },
    "typography": {
      "textAlign": true,
      "fontSize": true,
      "lineHeight": true
    }
  },
  "styles": [
    {
      "name": "prose",
      "label": "Prose"
    }
  ],
  "editorScript": "file:./js/edit.js",
