{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpbs/content-tabs-navigation",
  "title": "Content Tabs Nav",
  "description": "Display a tabs navigation",
  "selectors": {
    "root": ".wpbs-content-tabs-navigation"
  },
  "styles": [
    {
      "name": "vertical",
      "label": "Vertical"
    }
  ],
  "attributes": {
    "uniqueId": {
      "type": "string"
    }
  },
  "ancestor": [
    "wpbs/content-tabs"
  ],
  "usesContext": [
    "tabOptions",
    "tabActive",
    "tabPanels",
    "setTabActive"
  ],
  "supports": {
    "html": false,
    "innerBlocks": false,
    "dimensions": {
      "minHeight": true
    },
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "gradients": true
    },
    "spacing": {
      "blockGap": [
        "horizontal",
        "vertical"
      ],
      "padding": true,
      "margin": true
    },
    "typography": {
      "fontSize": true,
      "textAlign": true,
      "lineHeight": true,
      "__experimentalTextTransform": true,
      "__experimentalFontWeight": true
    },
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    }
  },
  "editorScript": "file:./block.js",
  "style": "file:./block.css",
  "icon": "welcome-widgets-menus"
}


