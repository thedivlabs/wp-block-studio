{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpbs/content-tabs-panel",
  "title": "Content Tabs Panel",
  "description": "Display a tabs panel",
  "selectors": {
    "root": ".wpbs-content-tabs-panel"
  },
  "attributes": {
    "uniqueId": {
      "type": "string"
    },
    "title": {
      "type": "string"
    }
  },
  "usesContext": [
    "activeTab"
  ],
  "parent": [
    "wpbs/content-tabs-container"
  ],
  "supports": {
    "html": false,
    "innerBlocks": true,
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "gradients": true
    },
    "spacing": {
      "blockGap": [
        "horizontal",
        "vertical"
      ],
      "padding": true,
      "margin": true
    },
    "typography": {
      "fontSize": true,
      "textAlign": true,
      "lineHeight": true,
      "__experimentalTextTransform": true,
      "__experimentalFontWeight": true
    },
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    }
  },
  "editorScript": "file:./block.js",
  "icon": "welcome-widgets-menus"
}


