{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpbs/content-tabs",
  "title": "Content Tabs",
  "description": "Display a tabs section",
  "selectors": {
    "root": ".wpbs-content-tabs"
  },
  "attributes": {
    "uniqueId": {
      "type": "string"
    }
  },
  "providesContext": {
    "wpbs/tab-options": "tabOptions",
    "wpbs/tab-panels": "tabPanels",
    "wpbs/tab-active": "tabActive"
  },
  "styles": [
    {
      "name": "fade",
      "label": "Fade"
    }
  ],
  "supports": {
    "interactivity": true,
    "html": false,
    "innerBlocks": true,
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "gradients": true
    },
    "spacing": {
      "blockGap": [
        "horizontal",
        "vertical"
      ],
      "padding": true,
      "margin": true
    },
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    }
  },
  "editorScript": "file:./block.js",
  "viewScriptModule": "file:./view.js",
  "style": "file:./block.css",
  "icon": "welcome-widgets-menus"
}


