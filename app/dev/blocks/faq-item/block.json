{
  "apiVersion": 3,
  "name": "wpbs/faq-item",
  "title": "FAQ Item",
  "description": "Display an FAQ group item",
  "selectors": {
    "root": ".wpbs-faq-item"
  },
  "parent": [
    "wpbs/faq-group"
  ],
  "attributes": {
    "uniqueId": {
      "type": "string"
    }
  },
  "allowedBlocks": [
    "wpbs/faq-header",
    "wpbs/faq-content"
  ],
  "usesContext": [
    "wpbs/group",
    "wpbs/faqItem"
  ],
  "styles": [
    {
      "name": "accordion",
      "label": "Accordion"
    }
  ],
  "supports": {
    "interactivity": true,
    "innerBlocks": true,
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "gradients": true
    },
    "spacing": {
      "blockGap": [
        "horizontal",
        "vertical"
      ],
      "padding": true,
      "margin": true
    },
    "shadow": true,
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    },
    "typography": {
      "__experimentalFontFamily": true,
      "__experimentalFontWeight": true,
      "textAlign": true,
      "fontSize": true,
      "lineHeight": true
    }
  },
  "editorScript": "file:./block.js",
  "style": "file:./block.css",
  "icon": "editor-kitchensink",
  "render": "file:./render.php"
}


