{
  "apiVersion": 3,
  "name": "wpbs/flyout-button",
  "title": "Flyout Button",
  "description": "Display the flyout toggle button",
  "selectors": {
    "root": ".wpbs-flyout-button"
  },
  "attributes": {
    "uniqueId": {
      "type": "string",
      "show_in_rest": true
    }
  },
  "supports": {
    "interactivity": true,
    "layout": false,
    "shadow": true,
    "color": {
      "background": true,
      "text": true,
      "link": false,
      "gradients": true
    },
    "__experimentalBorder": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "typography": {
      "__experimentalFontFamily": true,
      "__experimentalFontWeight": true,
      "__experimentalTextTransform": true,
      "textAlign": true,
      "fontSize": true,
      "lineHeight": true
    },
    "spacing": {
      "blockGap": [
        "horizontal",
        "vertical"
      ],
      "padding": true,
      "margin": true
    }
  },
  "editorScript": "file:./block.js",
  "icon": "button",
  "style": "file:./block.css"
}


