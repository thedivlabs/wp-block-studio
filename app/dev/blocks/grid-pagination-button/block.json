{
  "name": "wpbs/grid-pagination-button",
  "title": "Grid Button",
  "description": "Display a pagination button",
  "selectors": {
    "root": ".wpbs-grid-pagination-button"
  },
  "usesContext": [
    "label"
  ],
  "attributes": {
    "uniqueId": {
      "type": "string",
      "show_in_rest": true
    }
  },
  "supports": {
    "inserter": false,
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "gradients": true
    },
    "spacing": {
      "blockGap": [
        "horizontal",
        "vertical"
      ],
      "padding": true,
      "margin": true
    },
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    },
    "typography": {
      "__experimentalFontFamily": true,
      "__experimentalFontWeight": true,
      "textAlign": true,
      "fontSize": true,
      "lineHeight": true
    }
  },
  "editorScript": "file:./block.js",
  "icon": "grid-view",
  "render": "file:./render.php"
}


