{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpbs/icon-list-item",
  "title": "Icon List Item",
  "description": "Display a list item",
  "selectors": {
    "root": ".wpbs-icon-list-item"
  },
  "parent": [
    "wpbs/icon-list"
  ],
  "allowedBlocks": [
    "core/paragraph",
    "core/heading",
    "wpbs/icon-list"
  ],
  "attributes": {
    "uniqueId": {
      "type": "string"
    }
  },
  "supports": {
    "multiple": true,
    "reusable": false,
    "html": false,
    "innerBlocks": true,
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "gradients": true
    },
    "spacing": {
      "blockGap": [
        "horizontal",
        "vertical"
      ],
      "padding": true,
      "margin": true
    },
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    }
  },
  "editorScript": "file:./js/edit.js",
  "icon": "editor-ul"
}


