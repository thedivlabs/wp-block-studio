{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpbs/icon-list",
  "title": "Icon List",
  "description": "Display a list element",
  "selectors": {
    "root": ".wpbs-icon-list"
  },
  "allowedBlocks": [
    "wpbs/icon-list-item"
  ],
  "attributes": {
    "uniqueId": {
      "type": "string"
    }
  },
  "supports": {
    "innerBlocks": true,
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "gradients": true
    },
    "spacing": {
      "blockGap": [
        "horizontal",
        "vertical"
      ],
      "padding": true,
      "margin": true
    },
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    },
    "typography": {
      "textAlign": true,
      "lineHeight": true,
      "fontSize": true,
      "fontWeight": true,
      "fontStyle": true
    }
  },
  "editorScript": "file:./block.min.js",
  "style": "file:./block.css",
  "icon": "list-view"
}


