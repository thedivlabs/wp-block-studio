{
  "apiVersion": 3,
  "name": "wpbs/icon",
  "title": "Icon",
  "description": "Display an icon",
  "selectors": {
    "root": ".wpbs-icon-block"
  },
  "attributes": {
    "uniqueId": {
      "type": "string"
    }
  },
  "styles": [
    {
      "name": "image",
      "label": "Image"
    }
  ],
  "supports": {
    "color": {
      "background": true,
      "text": true,
      "gradients": true
    },
    "spacing": {
      "padding": true,
      "margin": true
    },
    "shadow": true,
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    }
  },
  "editorScript": "file:./block.js",
  "icon": "editor-kitchensink",
  "style": "file:./block.css"
}


