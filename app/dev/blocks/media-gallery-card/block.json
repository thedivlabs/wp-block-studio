  "selectors": {
    "root": ".wpbs-media-gallery-card"
  },
  "attributes": {
    "uniqueId": {
      "type": "string",
      "show_in_rest": true
    }
  },
  "parent": [
    "wpbs/media-gallery"
  ],
  "providesContext": {
    "wpbs/imageId": "imageId"
  },
