{
  "name": "wpbs/media-gallery",
  "title": "Media Gallery",
  "description": "Display a media gallery",
  "selectors": {
    "root": ".wpbs-media-gallery"
  },
  "attributes": {
    "uniqueId": {
      "type": "string",
      "show_in_rest": true
    },
    "galleryId": {
      "type": "integer",
      "show_in_rest": true
    }
  },
  "allowedBlocks": [
    "wpbs/media-gallery-card"
  ],
  "providesContext": {
    "postId": "postId"
  },
  "supports": {
    "layout": false,
    "interactivity": true,
    "innerBlocks": true,
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "gradients": true
    },
    "spacing": {
      "blockGap": [
        "horizontal",
        "vertical"
      ],
      "padding": true,
      "margin": true
    },
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    },
    "typography": {
      "__experimentalFontFamily": true,
      "__experimentalFontWeight": true,
      "textAlign": true,
      "fontSize": true,
      "lineHeight": true
    }
  },
  "editorScript": "file:./js/edit.js",
  "viewScriptModule": "file:./js/view.js",
  "style": "file:./js/edit.css",
  "editorStyle": "file:./js/edit.css",
  "icon": "format-gallery"
}


