{
  "name": "wpbs/media-gallery",
  "title": "Media Gallery",
  "description": "Display a media gallery",
  "selectors": {
    "root": ".wpbs-media-gallery"
  },
  "attributes": {
    "uniqueId": {
      "type": "string",
      "show_in_rest": true
    },
    "isSlider": {
      "type": "boolean",
      "show_in_rest": true
    }
  },
  "allowedBlocks": [
    "wpbs/media-gallery-card"
  ],
  "providesContext": {
    "wpbs/isSlider": "isSlider"
  },
  "styles": [
    {
      "label": "Grid",
      "name": "grid",
      "isDefault": true
    },
    {
      "label": "Slider",
      "name": "slider"
    }
  ],
  "supports": {
    "layout": false,
    "interactivity": true,
    "innerBlocks": true,
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "gradients": true
    },
    "spacing": {
      "blockGap": [
        "horizontal",
        "vertical"
      ],
      "padding": true,
      "margin": true
    },
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    },
    "typography": {
      "__experimentalFontFamily": true,
      "__experimentalFontWeight": true,
      "textAlign": true,
      "fontSize": true,
      "lineHeight": true
    }
  },
  "editorScript": "file:./block.js",
  "viewScriptModule": "file:./view.js",
  "style": "file:./block.css",
  "editorStyle": "file:./block.css",
  "icon": "format-gallery",
  "render": "file:./render.php"
}


