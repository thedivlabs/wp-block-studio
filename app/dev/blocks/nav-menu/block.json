{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpbs/nav-menu",
  "title": "Nav Menu",
  "description": "Display a menu",
  "selectors": {
    "root": ".wpbs-nav-menu"
  },
  "attributes": {
    "uniqueId": {
      "type": "string"
    }
  },
  "supports": {
    "html": false,
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "gradients": true
    },
    "spacing": {
      "blockGap": [
        "horizontal",
        "vertical"
      ],
      "padding": true,
      "margin": true
    },
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    }
  },
  "editorScript": "file:./block.min.js",
  "icon": "menu",
  "render": "file:./render.php"
}


