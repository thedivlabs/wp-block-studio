{
  "apiVersion": 3,
  "name": "wpbs/nav-menu",
  "title": "Nav Menu",
  "description": "Display a navigation menu",
  "selectors": {
    "root": ".wpbs-nav-menu"
  },
  "attributes": {
    "uniqueId": {
      "type": "string"
    }
  },
  "styles": [
    {
      "name": "columns",
      "label": "Columns"
    },
    {
      "name": "dropdown",
      "label": "Dropdown"
    },
    {
      "name": "accordion",
      "label": "Accordion"
    }
  ],
  "supports": {
    "interactivity": true,
    "innerBlocks": false,
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "gradients": true
    },
    "spacing": {
      "blockGap": [
        "horizontal",
        "vertical"
      ],
      "padding": true,
      "margin": true
    },
    "shadow": true,
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    },
    "typography": {
      "__experimentalFontFamily": true,
      "__experimentalFontWeight": true,
      "textAlign": true,
      "fontSize": true,
      "lineHeight": true
    }
  },
  "editorScript": "file:./block.js",
  "style": "file:./block.css",
  "icon": "editor-kitchensink"
}


