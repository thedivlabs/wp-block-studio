{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpbs/nav-menu",
  "title": "Nav Menu",
  "description": "Display a menu",
  "selectors": {
    "root": ".wpbs-nav-menu",
    "color": {
      "links": ".wpbs-nav-menu > .wpbs-nav-menu-container > li > a"
    }
  },
  "styles": [
    {
      "name": "dropdown",
      "label": "Dropdown",
      "isDefault": true
    },
    {
      "name": "accordion",
      "label": "Accordion"
    }
  ],
  "attributes": {
    "uniqueId": {
      "type": "string",
      "show_in_rest": true
    }
  },
  "supports": {
    "html": false,
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "gradients": true
    },
    "spacing": {
      "blockGap": [
        "horizontal",
        "vertical"
      ],
      "padding": true,
      "margin": true
    },
    "shadow": true,
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    }
  },
  "editorScript": "file:./block.js",
  "viewScriptModule": "file:./view.js",
  "style": "file:./block.css",
  "editorStyle": "file:./block.css",
  "icon": "menu",
  "render": "file:./render.php"
}


