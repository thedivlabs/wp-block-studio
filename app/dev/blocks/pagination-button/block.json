{
  "name": "wpbs/pagination-button",
  "title": "Grid Button",
  "description": "Display a pagination button",
  "selectors": {
    "root": ".wpbs-pagination-button",
    "spacing": {
      "root": ".wpbs-pagination-button",
      "padding": ".wpbs-pagination-button > a"
    },
    "color": ".wpbs-pagination-button > a",
    "border": ".wpbs-pagination-button > a"
  },
  "usesContext": [
    "settings"
  ],
  "attributes": {
    "uniqueId": {
      "type": "string",
      "show_in_rest": true
    }
  },
  "styles": [
    {
      "label": "Outline",
      "name": "outline"
    },
    {
      "label": "Reverse",
      "name": "reverse"
    },
    {
      "label": "Link",
      "name": "link"
    }
  ],
  "supports": {
    "interactivity": true,
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "gradients": true
    },
    "spacing": {
      "blockGap": [
        "horizontal",
        "vertical"
      ],
      "padding": true,
      "margin": true
    },
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    },
    "typography": {
      "__experimentalFontFamily": true,
      "__experimentalFontWeight": true,
      "textAlign": true,
      "fontSize": true,
      "lineHeight": true
    }
  },
  "editorScript": "file:./block.js",
  "icon": "grid-view"
}


