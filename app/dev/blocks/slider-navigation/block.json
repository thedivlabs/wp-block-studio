{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpbs/slider-navigation",
  "title": "Slider Navigation",
  "description": "Display the slider navigation element",
  "selectors": {
    "root": ".wpbs-slider-navigation"
  },
  "parent": [
    "wpbs/slider"
  ],
  "attributes": {
    "uniqueId": {
      "type": "string"
    }
  },
  "styles": [
    {
      "name": "group",
      "label": "Group"
    }
  ],
  "usesContext": [
    "wpbs/slider"
  ],
  "supports": {
    "typography": {
      "fontSize": true,
      "__experimentalLetterSpacing": true
    },
    "color": {
      "background": true,
      "text": false,
      "link": true,
      "gradients": true,
      "__experimentalSkipSerialization": true
    },
    "spacing": {
      "padding": true,
      "margin": true,
      "blockGap": [
        "horizontal"
      ]
    },
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    }
  },
  "editorScript": "file:./block.js",
  "style": [
    "file:./block.css"
  ],
  "icon": "slides"
}


