{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpbs/slider",
  "title": "Slider",
  "description": "Display a slider element",
  "selectors": {
    "root": ".wpbs-slider"
  },
  "attributes": {
    "uniqueId": {
      "type": "string"
    },
    "wpbs-transition": {
      "type": "string"
    },
    "styleCss": {
      "type": "string",
      "show_in_rest": true
    }
  },
  "allowedBlocks": [
    "wpbs/slider-wrapper",
    "wpbs/layout-element",
    "wpbs/layout-section",
    "wpbs/slider-navigation"
  ],
  "supports": {
    "innerBlocks": true,
    "interactivity": true,
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "gradients": true
    },
    "spacing": {
      "padding": true,
      "margin": true,
      "blockGap": [
        "horizontal"
      ]
    },
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    }
  },
  "editorScript": "file:./js/edit.js",
  "viewScriptModule": "file:./js/view.js",
  "style": "file:./js/edit.css",
  "icon": "slides"
}


