{
  "name": "wpbs/term-content",
  "title": "Term Content",
  "description": "Display term content",
  "selectors": {
    "root": ".wpbs-term-content"
  },
  "usesContext": [
    "wpbs/postId",
    "wpbs/termId",
    "wpbs/postType"
  ],
  "attributes": {
    "uniqueId": {
      "type": "string",
      "show_in_rest": true
    },
    "wpbs-term-content": {
      "type": "object"
    }
  },
  "supports": {
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "gradients": true
    },
    "spacing": {
      "padding": true,
      "margin": true
    },
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    },
    "typography": {
      "__experimentalFontFamily": true,
      "__experimentalFontWeight": true,
      "textAlign": true,
      "fontSize": true,
      "lineHeight": true
    }
  },
  "editorScript": "file:./block.js",
  "style": "file:./block.css",
  "editorStyle": "file:./block.css",
  "icon": "tag",
  "render": "file:./render.php"
}


