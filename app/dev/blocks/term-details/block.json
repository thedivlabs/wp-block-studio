{
  "name": "wpbs/term-details",
  "title": "Term Details",
  "description": "Display term details",
  "selectors": {
    "root": ".wpbs-term-details"
  },
  "usesContext": [
    "wpbs/termId"
  ],
  "styles": [
    {
      "name": "name",
      "label": "Name",
      "isDefault": true
    },
    {
      "name": "description",
      "label": "Description"
    },
    {
      "name": "thumbnail",
      "label": "Thumbnail"
    }
  ],
  "supports": {
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "gradients": true
    },
    "spacing": {
      "padding": true,
      "margin": true
    },
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    },
    "typography": {
      "__experimentalFontFamily": true,
      "__experimentalFontWeight": true,
      "textAlign": true,
      "fontSize": true,
      "lineHeight": true
    }
  },
  "editorScript": "file:./block.min.js",
  "style": "file:./block.css",
  "editorStyle": "file:./block.css",
  "icon": "tag",
  "render": "file:./render.php"
}


