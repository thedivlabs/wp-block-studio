{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpbs/video-element",
  "title": "Video Element",
  "description": "Display a video element",
  "selectors": {
    "root": ".wpbs-video"
  },
  "attributes": {
    "uniqueId": {
      "type": "string"
    },
    "preload": {
      "type": "array"
    },
    "styleCss": {
      "type": "string",
      "show_in_rest": true
    }
  },
  "supports": {
    "layout": false,
    "interactivity": true,
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "gradients": true
    },
    "spacing": {
      "padding": true,
      "margin": true
    },
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    }
  },
  "editorScript": "file:./js/edit.js",
  "viewScriptModule": "file:./js/view.js",
  "icon": "format-video",
  "render": "file:./render.php"
}


