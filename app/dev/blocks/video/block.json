{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpbs/video-element",
  "title": "Video Element",
  "description": "Display a video element",
  "selectors": {
    "root": ".wpbs-video"
  },
  "attributes": {
    "style": {
      "type": "object",
      "default": {
        "color": {
          "background": "#151a1fa1"
        }
      }
    }
  },
  "supports": {
    "layout": false,
    "interactivity": true,
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "gradients": true
    },
    "spacing": {
      "padding": true,
      "margin": true
    },
    "__experimentalBorder": {
      "size": true,
      "color": true,
      "radius": true
    }
  },
  "editorScript": "file:./block.js",
  "viewScriptModule": "file:./view.js",
  "icon": "format-video",
  "style": [
    "file:./block.css"
  ]
}