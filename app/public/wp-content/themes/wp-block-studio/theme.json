          "container": {
            "align": "inherit",
            "gap": {
              "row": ".8rem",
              "col": ".8rem",
              "wrapper": "2rem"
            }
          },
          "label": {
            "text-transform": "uppercase",
            "font-size": ".7rem",
            "color": "black",
            "weight": "normal",
            "gap": ".4rem"
          },
          "input": {
            "padding": ".2rem .8rem",
            "font-size": "17px !important",
            "height": "3.5rem",
            "color": {
              "background": "white",
              "text": "black",
              "placeholder": "gray"
            },
            "border": {
              "color": "white",
              "width": "1px",
              "radius": "0px",
              "style": "solid"
            },
            "focus": {
              "color": {
                "background": "white",
                "text": "black"
              },
              "border": {
                "color": "var:preset|color|primary-hover"
              }
            },
            "error": {
              "color": {
                "background": "#fff8f8",
                "text": "inherit",
                "placeholder": "red"
              },
              "border": {
                "color": "red"
              }
            }
          },
          "checkbox": {
            "height": "1.2rem",
            "icon": "",
            "color": {
              "background": "#fafafa",
              "text": "gray"
            },
            "border": {
              "width": "1px",
              "color": "var:preset|color|light",
              "style": "solid",
              "radius": "4px"
            },
            "active": {
              "icon": "\\f00c",
              "color": {
                "background": "var:preset|color|primary",
                "text": "white"
              },
              "border": {
                "color": "var:preset|color|primary"
              }
            }
          },
          "radio": {
            "icon": "'\\f00c'",
            "border": {
              "radius": "4px"
            }
          },
          "button": {
            "width": "auto",
            "height": "56px",
            "padding": ".2rem 1.5rem",
            "text-transform": "uppercase",
            "font-size": "inherit",
            "icon": "none",
            "color": {
              "background": "var:preset|color|primary",
              "text": "white",
              "icon": "inherit"
            },
            "border": {
              "color": "var:preset|color|primary",
              "width": "1px",
              "radius": "0"
            },
            "hover": {
              "color": {
                "background": "var:preset|color|primary-hover",
                "text": "white"
              },
              "border": {
                "color": "var:preset|color|primary-hover"
              }
            }
          }
        },
        "dark": {
          "label": {
            "color": "inherit",
            "weight": "normal",
            "gap": "5px"
          },
          "input": {
            "padding": "0 0 .4rem",
            "color": {
              "background": "transparent",
              "text": "white",
              "placeholder": "white"
            },
            "border": {
              "color": "var:preset|color|primary",
              "width": "0 0 1px",
              "style": "solid",
              "radius": "0"
            },
            "focus": {
              "color": {
                "background": "transparent"
              },
              "border": {
                "color": "var:preset|color|primary-hover"
              }
            },
            "error": {
              "color": {
                "background": "#fff8f8",
                "text": "inherit",
                "placeholder": "red"
              },
              "border": {
                "color": "red"
              }
            }
          },
          "checkbox": {
            "color": {
              "background": "transparent",
              "text": "gray"
            },
            "border": {
              "color": "var:preset|color|primary"
            },
            "active": {
              "color": {
                "background": "var:preset|color|primary",
                "text": "white"
              },
              "border": {
                "color": "var:preset|color|primary"
              }
            }
          },
          "button": {
            "icon": "\"\\f061\"",
            "color": {
              "background": "var:preset|color|primary-600",
              "text": "white",
              "icon": "white"
            },
            "border": {
              "color": "var:preset|color|primary-600"
            },
            "hover": {
              "color": {
                "background": "var:preset|color|primary-hover",
                "text": "white"
              },
              "border": {
                "color": "var:preset|color|primary-hover"
              }
            }
          }
        },
        "inline": {
          "gap": "0px"
        }
      },
      "loader": {
        "color": "var:preset|color|primary-hover",
        "background": "transparent",
        "opacity": "1"
      },
      "slider": {
        "bullet": {
          "background": "transparent",
          "gap": "5px",
          "height": "11px",
          "border": {
            "color": "var:preset|color|primary",
            "width": "1px",
            "style": "solid"
          },
          "active": {
            "background": "var:preset|color|primary"
          }
        },
        "button": {
          "next": "chevron_right",
          "prev": "chevron_left"
        }
      }
