        "typography": {
          "fontSize": "clamp(36px, 4rem, 5vw)",
          "lineHeight": "1.4em",
          "fontWeight": "700"
        }
      },
      "h2": {
        "css": "& strong {font-family:var(--wp--preset--font-family--playfair-display,inherit); font-size:1.3em; line-height:inherit; font-weight:300;}",
        "typography": {
          "fontSize": "clamp(28px, 3rem, 3.7vw)",
          "lineHeight": "1.4em",
          "fontWeight": "700"
        }
      },
      "h3": {
        "css": "& mark {display:inline-block; font-weight:300; font-size:clamp(18px,1.8rem, 20px); line-height:1.7em; text-transform:uppercase;}",
        "typography": {
          "fontSize": "clamp(20px, 1.8rem, 3vw)",
          "lineHeight": "1.4em",
          "fontWeight": "600"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "clamp(18px, 1.6rem, 2.5vw)",
          "lineHeight": "1.4em",
          "fontWeight": "normal"
        }
      },
      "heading": {
        "spacing": {
          "margin": {
            "top": "1.5rem",
            "bottom": "1rem"
          }
        }
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--montserrat)",
      "fontSize": "clamp(16px,4vw,18px)"
    },
    "blocks": {
      "core/post-terms": {
        "css": "& {display:flex; justify-content:flex-start; align-items:baseline; flex-wrap:wrap;} & .wp-block-post-terms__separator {margin:0 5px;}"
      },
      "core/categories": {
        "elements": {
          "link": {
            "css": "&[aria-current] {font-weight:bold;}"
          }
        }
      },
      "wpbs/authors-list": {
        "elements": {
          "link": {
            "css": "&[aria-current] {font-weight:bold;}"
          }
        }
      },
      "core/query-pagination": {
        "spacing": {
          "margin": {
            "top": "1.8rem"
          }
        }
      },
      "core/query-pagination-numbers": {
        "css": "gap:20px; & .pagination-button {font-size:1.5rem; line-height:1; color:var(--wp--preset--color--primary, inherit) !important; aspect-ratio:auto !important;}",
        "typography": {
          "textAlign": "center",
          "fontSize": "1rem",
          "lineHeight": "1"
        },
        "elements": {
          "button": {
            "dimensions": {
              "minHeight": "2rem",
              "aspectRatio": "1/1"
            },
            "color": {
              "background": "var:preset|color|primary",
              "text": "white"
            }
          },
          "link": {
            "color": {
              "background": "transparent",
              "text": "black"
            },
            "dimensions": {
              "minHeight": "1.7rem"
            },
            "spacing": {
              "padding": {
                "left": "8px",
                "right": "8px"
              }
            },
            ":hover": {
              "color": {
                "text": "var:preset|color|primary"
              }
            }
          }
        }
      },
      "wpbs/pagination-button": {
        "elements": {
          "link": {
            ":hover": {
              "color": {
                "background": "var:preset|color|primary-hover"
              },
              "border": {
                "color": "var:preset|color|primary-hover"
              }
            }
          }
        }
      },
      "wpbs/cta-button": {
        "variations": {
          "primary": {
            "css": "&:hover{background-color:transparent !important; color:var(--wp--preset--color--primary) !important; border-color:var(--wp--preset--color--primary) !important}"
          },
          "secondary": {
            "css": "&:hover{background-color:var(--wp--preset--color--primary-hover) !important; color:white !important}"
          },
          "outline": {
            "css": "&:hover{border-color:var(--wp--preset--color--primary-hover) !important; color:white !important; background-color:var(--wp--preset--color--primary-hover) !important}"
          },
          "reverse": {
            "css": "&:hover{border-color:var(--wp--preset--color--primary-hover) !important;  color:white !important}"
          },
          "link": {
            "css": "&:hover{border-color:var(--wp--preset--color--primary-hover) !important;"
          }
        }
      }
    }
  }
}
