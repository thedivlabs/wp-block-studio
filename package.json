{
  "devDependencies": {
    "@wordpress/block-editor": "^14.4.0",
    "@wordpress/blocks": "^13.9.0",
    "@wordpress/components": "^28.9.0",
    "@wordpress/compose": "^7.9.0",
    "@wordpress/core-data": "^7.9.0",
    "@wordpress/data": "^10.9.0",
    "@wordpress/element": "^6.25.0",
    "@wordpress/hooks": "^4.29.0",
    "@wordpress/icons": "^10.9.0",
    "@wordpress/interactivity": "^6.24.0",
    "@wordpress/scripts": "^30.7.0",
    "postcss-nesting": "^13.0.2"
  },
  "scripts": {
    "wp-build": "wp-scripts build --webpack-src-dir=./app/dev/blocks/ --output-path=./app/public/wp-content/plugins/wp-block-studio/build/ --experimental-modules",
    "wp-dev": "wp-scripts start --webpack-src-dir=./app/dev/blocks/ --output-path=./app/public/wp-content/plugins/wp-block-studio/build/ --experimental-modules",
    "tailwind-theme-dev": "npx @tailwindcss/cli -i ./app/dev/scss/theme.css -o ./app/public/wp-content/plugins/wp-block-studio/build/theme.css --watch",
    "tailwind-admin-dev": "npx @tailwindcss/cli -i ./app/dev/scss/admin.css -o ./app/public/wp-content/plugins/wp-block-studio/build/admin.css --watch",
    "tailwind-theme": "npx @tailwindcss/cli -i ./app/dev/scss/theme.css -o ./app/public/wp-content/plugins/wp-block-studio/build/theme.css --minify",
    "tailwind-admin": "npx @tailwindcss/cli -i ./app/dev/scss/admin.css -o ./app/public/wp-content/plugins/wp-block-studio/build/admin.css --minify",
    "start": "npm-run-all --parallel wp-dev tailwind-theme-dev tailwind-admin-dev",
    "build": "npm-run-all --parallel wp-build tailwind-theme tailwind-admin"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.0.6",
    "@tailwindcss/postcss": "^4.1.11",
    "@wordpress/api-fetch": "^7.28.0",
    "@wordpress/date": "^5.32.0",
    "@wordpress/i18n": "^6.7.0",
    "@wordpress/server-side-render": "^6.3.0",
    "autoprefixer": "^10.4.21",
    "lodash": "^4.17.21",
    "lodash.merge": "^4.6.2",
    "mini-css-extract-plugin": "^2.9.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "react": "^18.3.1",
    "tailwindcss": "latest"
  }
}
